<?xml version="1.0" encoding="utf-8"?>
<process>
    <uid>DIALER</uid>
    <desc_name>Call establisher for server queue</desc_name>
    <exec_priority>0</exec_priority>
    <dependences>
        <dependence>
            <uid>QA01</uid>
            <tags>
                <tag>QUEUE_EMPTY</tag>
            </tags>
            <exceptions>
                <exception>
                    <uid>QA01</uid>
                    <desc>Queue must be empty</desc>
                    <tags>
                        <tag>
                            <id>QUEUE_EMPTY</id>
                            <rep>REPLACED01</rep>
                        </tag>
                        <tag>
                            <id>QUEUE_EMPTY</id>
                            <rep>REPLACED02</rep>
                        </tag>
                    </tags>
                </exception>
                <exception>
                    <uid>QA02</uid>
                    <desc>Queue must exist</desc>
                    <tags>
                        <tag>
                            <id>QUEUE_EMPTY</id>
                            <rep>REPLACED03</rep>
                        </tag>
                    </tags>
                </exception>
            </exceptions>
        </dependence>
        <dependence>
            <uid>FD01</uid>
            <tags>
                <tag>ARMFILE</tag>
            </tags>
        </dependence>
    </dependences>
    <commands>
        <command>
            <uid>DIALER01</uid>
            <exec>com.epitools.exe.dialer</exec>
            <input>in.txt</input>
            <output>out.com</output>
            <argvs>
                <arg>-cn localhost</arg>
                <arg>-id sessionname</arg>
                <arg>-a 0</arg>
            </argvs>
            <tags>
                <tag>DIALER_CONNECTION</tag>
            </tags>
        </command>
        <command>
            <uid>DIALER02</uid>
            <exec>touch</exec>
            <input>in.txt</input>
            <output>out.com</output>
            <tags>
                <tag>STATUS_FILE</tag>
            </tags>
        </command>
    </commands>
</process>